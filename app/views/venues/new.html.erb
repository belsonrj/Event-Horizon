<h1>New Venue</h1>

<br />

<%= form_for :venue, url: venues_path do |f| %>
  <%= f.label "Name" %> <br>
  <%= f.text_field :name %> <br>
  <%= f.label "Locale" %> <br>
  <%= f.text_field :locale %> <br>
  <%= f.label "Venue Type" %> <br>
  <%= f.select :venue_type, [ nil, 'Bar', 'Small', 'Medium', 'Large', 'Arena', 'Festival', 'Amphitheater', 'Pavillion', 'Outdoor', 'Theatre']%><br>
  <%= f.label "Venue Comments" %> <br>
  <%= f.text_area :content %><br>

  <br>

  <%= f.submit %>
<% end %>



      <% if logged_in? %>
        <h1>You are Logged In, <%= current_user.username %></h1>
      <%end%>