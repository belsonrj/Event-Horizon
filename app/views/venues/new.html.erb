<h1>New Venue</h1>

<br />

<%= form_for @venue  do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %> <br>
  <%= f.label "Locale" %>
  <%= f.text_field :locale %> <br>
  <%= f.label "Venue Type" %>
  <%= f.text_field :venue_type %><br>

  <br>

  <%= f.submit %>
<% end %>



      <% if logged_in? %>
        <h1>You are Logged In, <%= current_user.username %></h1>
      <%end%>