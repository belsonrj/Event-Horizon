<!DOCTYPE html>
<html>
  <body>
    <h1 id=title>Artists You've Met</h1>
      
      <%= button_to "Home", user_path(@user), {:class => "button"} %>
    
       <h2 id=top> Artist Count: <%= @artist_met.count %></h2>

       <ol id="list">
       <% @artist_met.order_artists.each do |artist| %>
        <li><%= link_to artist.name, user_artist_path(@user, artist) %></li>
       <% end %>
       </ol>

  </body>
</html>