<!DOCTYPE html>
<html>
  <body>
      <h1 id=title>Artists You've Met</h1>
      <%= link_to 'Back', user_path(@user), {:class => "button"} %>

      <div class="box">
        <h2 id=top> Artist Count: <%= @artist_met.count %></h2>
        <ol id="list">
          <% @artist_met.order_artists.each do |artist| %>
            <li><%= link_to artist.name, user_artist_path(@user, artist) %></li><br>
          <% end %>
        </ol>
      </div>
  </body>
</html>