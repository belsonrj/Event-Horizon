<!DOCTYPE html>
<html>
<h1>Sign Up!</h1>

  <p>
    <%= form_for(@user) do |f| %>
      <%= f.label :username%>
      <%= f.text_field :username%><br>
      <br>
      <%= f.label :password%>
      <%= f.password_field :password%><br>
      <br>
      <%= f.submit "Sign Up"%>
    <% end %>
  </p>
</html>